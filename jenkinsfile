pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
               mvn "build phase completed successfully"
            }
        }
        stage('Test') {
            steps {
               mvn "test phase completed successfully"
            }
        }
        stage('Package') {
            steps {
               mvn "package new completed successfully"
            }
        }
  
    }
}
